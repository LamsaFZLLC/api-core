{
    "name": "lamsa/api-core",
    "description": "Api Core Component for Php Symfony projects",
    "authors": [
        {
            "name": "Zaid Sasa",
            "email": "zaidsa3sa3@gmail.com"
        }
    ],
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2.5",
        "bugsnag/bugsnag-symfony": "^1.13",
        "doctrine/collections": "^1.8",
        "friendsofsymfony/rest-bundle": "^3.6",
        "symfony/event-dispatcher": "5.4.*",
        "symfony/flex": "^1.17|^2",
        "symfony/form": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/validator": "5.4.*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true,
            "symfony/flex": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Lamsa\\ApiCore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Lamsa\\ApiCore\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "symfony/phpunit-bridge": "^6.3"
    }
}
